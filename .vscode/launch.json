    "version": "0.2.0",
    "configurations": [

        {
            "name": "Python Debugger: Module",
            "type": "debugpy",
            "request": "launch",
            "module": "grpc_temp",
            // set working directory as src
            "cwd": "${workspaceFolder}/src",
        },
        {
            "name": "Python: CLI",
            "type": "python",
            "request": "launch",
            "module": "grpc_temp",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src",
            "args": [""]
        }
    ]
}